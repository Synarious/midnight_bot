{
  "name": "midnight-bot-dashboard",
  "version": "1.0.0",
  "description": "Web dashboard for Midnight Bot administration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "vite --config vite.config.mjs",
    "build": "vite build --config vite.config.mjs",
    "preview": "vite preview --config vite.config.mjs",
    "server": "node server.js",
    "docker:dev": "concurrently -k \"vite --config vite.config.mjs\" \"nodemon -L server.js --watch server.js\""
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@nivo/line": "^0.99.0",
    "axios": "^1.6.2",
    "chart.js": "^4.4.1",
    "clsx": "^2.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "notistack": "^3.0.1",
    "pg": "^8.11.3",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}
